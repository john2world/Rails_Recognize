<% if current_user.blank? %>

  <li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><%= t('dict.signin')%> <span class="caret"></span></a>
    <div class="dropdown-menu" role="menu">
      <%= render :partial => "user_sessions/login" %>
    </div>
  </li>
  <li>
    <%= link_to "Sign up", signup_path, data: {"no-turbolink" => true} %>
    <a href="/sign-up" data-no-turbolink='true'>Sign up</a>
  </li>
<% else %>
  <li>
    <%= link_to "Back to #{current_user.company.name}", stream_path %>
  </li>
<% end %>